{
  "name": "yunaforseyfert",
  "version": "1.0.0",
  "description": "Yunathings for seyfert",
  "type": "commonjs",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "private": true,
  "files": ["dist"],
  "scripts": {
    "bot": "cd src/bot-test && bun .",
    "yuna": "vitest run test && mkdir -p .npm && npm run build && bun src/package/lib/prepublish.ts && cp -r build .npm/build",
    "prepare": "npm run build && npx husky",
    "build": "npx tsup --config tsup.config.json",
    "lint": "biome lint --write ./src",
    "format": "biome check --write ./src",
    "test": "vitest"
  },
  "keywords": [
    "parser",
    "penguin"
  ],
  "author": "SagiriIkeda",
  "license": "ISC",
  "dependencies": {
    "discord-api-types": "^0.37.90",
    "seyfert": "github:tiramisulabs/seyfert"
  },
  "homepage": "https://github.com/SagiriIkeda/yunaforseyfert#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SagiriIkeda/yunaforseyfert.git"
  },
  "lint-staged": {
    "*.ts": [
      "biome check --write",
      "biome format --write"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.0",
    "@types/node": "^20.14.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "tsup": "^8.1.0",
    "typescript": "^5.0.0",
    "vitest": "^1.5.2"
  }
}
